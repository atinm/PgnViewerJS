{
  "name": "@atinm/pgn-viewer",
  "description": "A pgn viewer to view or edit chess games on HTML web sites.",
  "keywords": [
    "chess",
    "pgn",
    "viewer"
  ],
  "version": "1.4.2",
  "main": "lib/pgnv.js",
  "files": [
    "lib/pgnv-assets/**",
    "lib/locales/**",
    "README.md"
  ],
  "module": "modules/pgn-viewer/src/index.js",
  "author": "atinm",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atinm/PgnViewerJS.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "../../node_modules/.bin/webpack --config webpack.prod.js",
    "start": "../../node_modules/.bin/webpack-dev-server --open --config webpack.dev.js",
    "test": "cd ../../ && ./node_modules/.bin/mocha ./modules/pgn-viewer/test/**/*.js",
    "test-interactive": "cd ../../ && ./node_modules/.bin/mocha -w  ./modules/pgn-viewer/test/**/*.js",
    "gen_i18n": "node ./bin/gen-i18n.js"
  },
  "dependencies": {
    "@atinm/pgn-reader": "file:../pgn-reader",
    "chessground": "^7.9.3",
    "mousetrap": "^1.6.5",
    "roddeh-i18n": "^1.2.0",
    "smoothscroll-polyfill": "^0.4.4",
    "sweetalert": "^2.1.2"
  },
  "devDependencies": {
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
